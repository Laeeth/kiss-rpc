	
	//kiss rpc idl demo

	@message:user_info
	{
		string phone:3;
		string user_name:1;
		int age:2;
		double wiget:4;
		
		string[] address_list:5;
	}

	@message:contacts
	{
		int number:1;
		user_info[] user_info_list:2;		
	}


	@service:address_book	//接口类
	{
		sync: contacts sync_get_contact_list(string account_name);	//同步
		async: contacts async_get_contact_list(string account_name);	//异步
	}

